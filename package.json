{
	"name": "eiram-next",
	"version": "0.1.0",
	"private": true,
	"license": "SEE LICENSE IN LICENSE.md",
	"scripts": {
		"dev": "NODE_OPTIONS='--trace-warnings' next dev -p 8141",
		"build": "NODE_OPTIONS='--trace-warnings' next build",
		"start": "next start -p 8141",
		"lint": "next lint",
		"codegen": "graphql-codegen -r dotenv/config -c lib/api/codegen/config.ts -w ",
		"pm2-dev": "pm2 start npm --name \"Eiram NextJS Dev\" -- run dev",
		"pm2-dev-stop": "pm2 stop \"Eiram NextJS Dev\" && pm2 delete \"Eiram NextJS Dev\"",
		"pm2-dev-restart": "pm2 restart \"Eiram NextJS Dev\"",
		"pm2-start": "pm2 start npm --name \"Eiram NextJS Production\" -- run start",
		"pm2-stop": "pm2 stop \"Eiram NextJS Production\" && pm2 delete \"Eiram NextJS Production\"",
		"pm2-restart": "pm2 restart \"Eiram NextJS Production\"",
		"pm2-node-start": "PORT=8141 pm2 start --name \"Eiram Node\" node dist/index.js",
		"pm2-node-stop": "pm2 stop --name \"Eiram Node\" && pm2 delete \"Eiram Node\"",
		"pm2-node-restart": "pm2 restart \"Eiram Node\""
	},
	"dependencies": {
		"@headlessui/react": "^1.7.10",
		"@heroicons/react": "^2.0.15",
		"@hookform/error-message": "^2.0.1",
		"@hookform/resolvers": "^2.9.11",
		"@next/font": "^13.1.6",
		"@stripe/react-stripe-js": "^1.16.4",
		"@stripe/stripe-js": "^1.46.0",
		"@xstate/react": "^3.2.1",
		"cookies-next": "^2.1.1",
		"graphql": "^16.6.0",
		"graphql-request": "^5.1.0",
		"html-react-parser": "^3.0.9",
		"jwt-decode": "^3.1.2",
		"jwt-encode": "^1.0.1",
		"next": "^13.1.7-canary.14",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.43.1",
		"sharp": "^0.31.3",
		"stripe": "^11.10.0",
		"xstate": "^4.36.0",
		"zod": "^3.20.6"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^3.0.0",
		"@graphql-codegen/client-preset": "^2.0.0",
		"@graphql-codegen/gql-tag-operations-preset": "^2.0.0",
		"@graphql-codegen/typescript": "^3.0.0",
		"@graphql-codegen/typescript-operations": "^3.0.0",
		"@tailwindcss/forms": "^0.5.3",
		"@types/jwt-encode": "^1.0.0",
		"@types/node": "18.13.0",
		"@types/react": "18.0.28",
		"@types/react-dom": "18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"autoprefixer": "^10.4.13",
		"encoding": "^0.1.13",
		"eslint": "8.34.0",
		"eslint-config-next": "^13.1.6",
		"postcss": "^8.4.21",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.4",
		"tailwindcss": "^3.2.6",
		"typescript": "^4.9.5"
	}
}
